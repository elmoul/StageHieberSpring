<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	 <class name="src.dao.User" table="USER">
	 	<id name="id" column="ID_USER">
	 		<generator class="native"></generator>
	 	</id>
	 	<property name="nom" column="NOM"></property>
	 	<property name="prenom" column="PRENOM"></property>
	 	<property name="email" column="EMAIL"></property>
	 	<property name="role" column="ROLE"></property>
	 	<property name="type" column="TYPE"></property>
	 	<property name="tel" column="TEL"></property>
	 	<property name="adresse" column="ADRESSE"></property>
	 	<property name="ville" column="VILLE"></property>
	 	<property name="username" column="USERNAME"></property>
	 	<property name="password" column="PASSWORD"></property>
	
	 
	 </class>
  
</hibernate-mapping>